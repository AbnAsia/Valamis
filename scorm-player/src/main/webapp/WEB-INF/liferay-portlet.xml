<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 5.2.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_5_2_0.dtd">
<liferay-portlet-app>
    <portlet>
        <portlet-name>SCORMApplication</portlet-name>
        <instanceable>true</instanceable>
        
        <header-portlet-css>/css/custom-theme/jquery-ui-1.8.10.custom.css</header-portlet-css>
        <header-portlet-css>/css/ui.jqgrid.css</header-portlet-css>
        <header-portlet-css>/css/fileuploader.css</header-portlet-css>
        <header-portlet-css>/css/jstree/style.css</header-portlet-css>
        
        <footer-portlet-javascript>/js/grid.locale-en.js</footer-portlet-javascript>
        <footer-portlet-javascript>/js/jquery.jqGrid.min.js</footer-portlet-javascript>
        <footer-portlet-javascript>/js/jquery.jstree.js</footer-portlet-javascript>
        <footer-portlet-javascript>/js/jquery.i18n.properties-min.js</footer-portlet-javascript>
        <footer-portlet-javascript>/js/fileuploader.js</footer-portlet-javascript>
    </portlet>
    <role-mapper>
        <role-name>administrator</role-name>
        <role-link>Administrator</role-link>
    </role-mapper>
    <role-mapper>
        <role-name>guest</role-name>
        <role-link>Guest</role-link>
    </role-mapper>
    <role-mapper>
        <role-name>power-user</role-name>
        <role-link>Power User</role-link>
    </role-mapper>
    <role-mapper>
        <role-name>user</role-name>
        <role-link>User</role-link>
    </role-mapper>
</liferay-portlet-app>
