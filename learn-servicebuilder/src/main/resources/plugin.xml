                <liferayVersion implementation="java.lang.String">${liferayVersion}</liferayVersion>
                <localArtifactRepository implementation="org.apache.maven.artifact.repository.ArtifactRepository">${localRepository}</localArtifactRepository>
                <pluginName implementation="java.lang.String" default-value="${project.artifactId}">${pluginName}</pluginName>
                <pluginType implementation="java.lang.String" default-value="portlet">${pluginType}</pluginType>
                <postBuildDependencyModules implementation="boolean" default-value="false">${postBuildDependencyModules}</postBuildDependencyModules>
                <project implementation="org.apache.maven.project.MavenProject">${project}</project>
                <remoteArtifactRepositories implementation="java.util.List">${project.remoteArtifactRepositories}</remoteArtifactRepositories>
                <serviceBuildNumber implementation="long" default-value="1">${serviceBuildNumber}</serviceBuildNumber>
                <serviceBuildNumberIncrement implementation="boolean" default-value="true">${serviceBuildNumberIncrement}</serviceBuildNumberIncrement>
