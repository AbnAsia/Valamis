                <sqlIndexesPropertiesFileName implementation="java.lang.String" default-value="indexes.properties"/>
                <sqlSequencesFileName implementation="java.lang.String" default-value="sequences.sql"/>
                <workDir implementation="java.io.File" default-value="${project.build.directory}/liferay-work"/>
